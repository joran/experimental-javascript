{
  "name": "node-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dbstart": "mongod --dbpath data/db --fork --logpath /tmp/mongod.log",
    "start": "node ./bin/www",
    "stop": "echo 'bye!'",
    "dbstop": "mongo admin --eval 'db.shutdownServer()' >> /tmp/mongod.log"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "mongodb": "~2.0.47",
    "monk": "~1.0.1"
  }
}
